CREATE TABLE TRADEMARK(APPLICATIONNO VARCHAR(20) NOT NULL,APPLICATIONDATE VARCHAR(30),REGISTERNO VARCHAR(20),REGISTERDATE VARCHAR(30),INTREGNO VARCHAR(20),NAME VARCHAR(500),NICECLASSES VARCHAR(255),VIENNACLASSES VARCHAR(255),TYPE VARCHAR(1),PUBTYPE VARCHAR(1),CONSTRAINT PK_TRADEMARK0 PRIMARY KEY(APPLICATIONNO))
CREATE INDEX TMNAME ON TRADEMARK(NAME)
CREATE TABLE HOLDER(APPLICATIONNO VARCHAR(20),TPECLIENTID VARCHAR(20),TITLE VARCHAR(255),ADDRESS VARCHAR(500),STATE VARCHAR(100),POSTALCODE VARCHAR(20),CITY VARCHAR(60),COUNTRYNO VARCHAR(60))
CREATE INDEX HOLDERTITLE ON HOLDER(TITLE)
CREATE TABLE GOODS(APPLICATIONNO VARCHAR(20),CLASSID VARCHAR(3),SUBCLASSID VARCHAR(3),GOODS VARCHAR(4000))
CREATE TABLE EXTRACTEDGOODS(APPLICATIONNO VARCHAR(20),CLASSID VARCHAR(3),SUBCLASSID VARCHAR(3),GOODS VARCHAR(4000))
CREATE TABLE ATTORNEY(APPLICATIONNO VARCHAR(20),NO VARCHAR(20),NAME VARCHAR(64),TITLE VARCHAR(255))
CREATE INDEX ATTORNEYNAME ON ATTORNEY(NAME)
CREATE USER SA PASSWORD "" ADMIN
